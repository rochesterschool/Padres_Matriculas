            <key-property name="valor" type="string">
                <column name="valor"/>
            </key-property>
            <key-property name="pordcto" type="string">
                <column name="pordcto"/>
            </key-property>
            <key-property name="valdcto" type="string">
                <column name="valdcto"/>
            </key-property>
            <key-property name="anticipo" type="string">
                <column name="anticipo"/>
            </key-property>
            <key-property name="saldo" type="string">
                <column name="saldo"/>
            </key-property>
            <key-property name="nnombre" type="string">
                <column name="nnombre"/>
            </key-property>
            <key-property name="cursoact" type="string">
                <column name="cursoact"/>
            </key-property>
            <key-property name="alu" type="string">
                <column name="alu"/>
            </key-property>
            <key-property name="otro" type="string">
                <column name="otro"/>
            </key-property>
        </composite-id>
    </class>
</hibernate-mapping>