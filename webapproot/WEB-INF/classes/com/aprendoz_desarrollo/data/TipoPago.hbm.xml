<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.aprendoz_desarrollo.data.TipoPago" table="tipo_pago" catalog="aprendoz_desarrollo" dynamic-insert="false" dynamic-update="false">
        <id name="idTipoPago" type="integer">
            <column name="id_tipo_pago"/>
            <generator class="identity"/>
        </id>
        <property name="tipoPago" type="string">
            <column name="tipo_pago" length="100" not-null="true"/>
        </property>
        <set name="inscPersonaEduComs" inverse="true" cascade="">
            <key>
                <column name="tipo_pago_id_tipo_pago" not-null="true"/>
            </key>
            <one-to-many class="com.aprendoz_desarrollo.data.InscPersonaEduCom"/>
        </set>
    </class>
</hibernate-mapping>